<configs>
  <!-- Daily Friens -->
  <set xpath="/spawning/biome[@name='pine_forest']/spawn[starts-with (@entitygroup,'Zombies')]/@respawndelay">1</set>
  <set xpath="/spawning/biome[@name='burnt_forest']/spawn[starts-with (@entitygroup,'Zombies')]/@respawndelay">1</set>
  <set xpath="/spawning/biome[@name='desert']/spawn[starts-with (@entitygroup,'Zombies')]/@respawndelay">1</set>
  <set xpath="/spawning/biome[@name='snow']/spawn[starts-with (@entitygroup,'Zombies')]/@respawndelay">1</set>

  <!-- Moar Night Friens -->
  <set xpath="/spawning/biome[@name='pine_forest']/spawn[@entitygroup='ZombiesNight']/@maxcount">3</set>
  <set xpath="/spawning/biome[@name='burnt_forest']/spawn[@entitygroup='ZombiesNight']/@maxcount">3</set>
  <set xpath="/spawning/biome[@name='desert']/spawn[@entitygroup='ZombiesNight']/@maxcount">3</set>
  <set xpath="/spawning/biome[@name='wasteland']/spawn[@entitygroup='ZombiesNight']/@maxcount">4</set>
  <set xpath="/spawning/biome[@name='snow']/spawn[@entitygroup='ZombiesNight']/@maxcount">3</set>
</configs>


